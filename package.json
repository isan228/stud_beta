{
  "name": "stud.kg",
  "version": "1.0.0",
  "description": "Платформа для подготовки студентов к экзаменам",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "init-db": "node scripts/initDatabase.js",
    "create-admin": "node scripts/createAdmin.js",
    "setup-postgres": "bash scripts/setupPostgres.sh",
    "fix-postgres": "bash scripts/fixPostgresUser.sh",
    "check-server": "bash scripts/checkServer.sh",
    "restart-server": "bash scripts/restartServer.sh",
    "setup-ssl": "bash scripts/setupSSL.sh",
    "test-admin": "node scripts/testAdminLogin.js",
    "fix-admin": "node scripts/fixAdminAndJWT.js",
    "add-subjects": "node scripts/addSubjects.js",
    "add-tests": "node scripts/addTests.js",
